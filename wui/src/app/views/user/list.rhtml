      <% @page_title = "View virt-factory Users" %>
      <%= ApplicationHelper.menubar("user","list") %>
     
      <div id="content-main">
      <TABLE class='listing'>
         <TR>
            <TD>Username</TD>
            <TD>First</TD>
            <TD>Middle</TD>
            <TD>Last</TD>
            <TD>Description</TD>
            <TD>Email</TD>
            <TD></TD>
            <TD></TD>
         </TR>
      <%# FIXME: this may be a result of what objects are being sent over from Python. %>
      <%# FIXME: web svc should return a hash not a list %>
      <% @items.each do |item| %>
         <TR>
             
             <TD> <%= item.username %> </TD>
             <TD> <%= item.first %> </TD>
             <TD> <%= item.middle %> </TD>
             <TD> <%= item.last %> </TD>
             <TD> <%= item.description %> </TD>
             <TD> <%= item.email %> </TD>
             <TD> <%= link_to "[edit]",   :controller => "user", :action=> "edit",   :id=> item.id %></TD>
             <TD> <%= link_to "[delete]", :controller => "user", :action=> "delete", :id=> item.id %></TD>
         </TR>
      <% end %>
      </TABLE>
      </div>

